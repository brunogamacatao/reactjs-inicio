{
  "scripts": {
    "start": "webpack-dashboard -- node server.js",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config ./config/webpack.prod -p",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "deepmerge": "^1.3.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "svg-url-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.1.0",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-prop-types": "^0.4.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass|less|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/config/**",
      "!**/__mocks__/**",
      "!**/coverage/**",
      "!**/storybook/**",
      "!**/server.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTestFramework.js"
    ]
  }
}
